
public class MyDate {
	int dd;
	int mm;
	int yy;
	
	//default constructor
	MyDate(){
		System.out.println("insde MyDate()");
		dd=1;
		mm=1;
		yy=2001;
	}
	
	MyDate(int d, int m, int y){
		System.out.println("insde MyDate(int,int,int)");
		dd=d;
		mm=m;
		yy=y;
	}
	
	void init(int d, int m, int y) {
		System.out.println("inside init method");
		dd=d;
		mm=m;
		yy=y;
	}
	
	void printDate() {
		System.out.println(dd+"/"+mm+"/"+yy);
	}

}
