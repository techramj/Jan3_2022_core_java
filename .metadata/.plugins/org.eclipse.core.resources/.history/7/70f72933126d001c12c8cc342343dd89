
public class Main {
	
	public static void main(String[] args) {
		printPattern1();
	}
	
/*
1
12
123
1234
12345
*/
	public static void printPattern1() {
		int k=0;
		for(int i=1;i<=5;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(++k);
				if(k==9) {
					k=0;
				}
			}
			System.out.println();
		}
	}

}
