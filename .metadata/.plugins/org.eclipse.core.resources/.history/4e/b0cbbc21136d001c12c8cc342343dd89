
public class Main {
	
	public static void main(String[] args) {
		printPattern1();
	}
	
/*
1
12
123
1234
12345
*/
	public static void printPattern1() {
		int k=0;
		for(int i=1;i<=5;i++) {
			for(int j=1;j<=i;j++) {
				int y=(++k)%10;
				System.out.print(y);
			}
			System.out.println();
		}
	}

}


